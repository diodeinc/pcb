load("@stdlib/properties.zen", "Layout")

Capacitor = Module("@stdlib/generics/Capacitor.zen")

# Define module inputs/outputs
power = io("power", Net)
gnd = io("gnd", Net)

# Create two capacitors in this module
# First capacitor - decoupling cap
Capacitor(
    name = "C1",
    package = "0402",
    P1 = power,
    P2 = gnd,
)

# Second capacitor - bulk cap
Capacitor(
    name = "C2",
    package = "0603",
    P1 = power,
    P2 = gnd,
)

Layout(name="Module", path="build/module_layout", bom_profile=None)
