load("stdlib.zen", "Ground", "I2C", "Power")
load("units.zen", "Capacitance")

Capacitor = Module("Capacitor.zen")

power = io("power", Power)
i2c = io("i2c", I2C)

Component(
    name = "IC",
    pins = {
        "VDD": power.vcc,
        "GND": Ground,
        "SCX": i2c.scl,
        "SDX": i2c.sda,
        "ASCX": i2c.scl,
        "ASDX": i2c.sda,
        "CSB": Net(),
        "GNDIO": Ground,
        "INT1": Net(),
        "INT2": Net(),
        "OCSB": Net(),
        "OSDO": Net(),
        "SDO": Net(),
        "VDDIO": power.vcc,
    },
    symbol = Symbol(library = "eda/BMI270.kicad_sym", name = "BMI270"),
    footprint = "eda/BMI270.kicad_mod",
)

Capacitor(
    name = "C",
    P1 = power.vcc,
    P2 = Ground,
    package = "0402",
    value = 0.0,
)
