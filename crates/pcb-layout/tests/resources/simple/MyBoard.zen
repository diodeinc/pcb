load("@stdlib/interfaces.zen", "I2c", "Power", "Ground")
load("@stdlib/units.zen", "Capacitance")
load("@stdlib/properties.zen", "Layout")

BMI270 = Module("BMI270.zen")

power = Power("POWER")
gnd = Ground("GND")
i2c = I2c("I2C")

BMI270(
    name = "BMI270",
    POWER = power,
    GND = gnd,
    I2C = i2c,
)

Layout(name="MyBoard", path="build/", bom_profile=None)
