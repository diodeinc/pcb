load("@stdlib/interfaces.zen", "I2c", "Power", "Ground")
load("@stdlib/units.zen", "Capacitance")

BMI270 = Module("BMI270.zen")

power = Power("POWER")
gnd = Ground("GND")
i2c = I2c("I2C")

BMI270(
    name = "BMI270",
    POWER = power,
    GND = gnd,
    I2C = i2c,
)

# HACK: Put the layout in build/ so that the snapshot ends up there, which
# is what the test expects.
add_property("layout_path", "build/")
