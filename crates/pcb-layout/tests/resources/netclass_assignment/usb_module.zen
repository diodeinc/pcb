load("@stdlib:v0.3.4/interfaces.zen", "Usb2")

# Accept Usb2 interface as input
# When parent passes a Usb2 with DiffPair(impedance=90Î©) to this module,
# propagate_diffpair_impedance() will walk the module signature and propagate
# the impedance to D.P and D.N nets
usb = io("usb", Usb2)

# Add a simple component inside the module to make it non-empty
Component(
    name="R_USB",
    footprint="Resistor_SMD:R_0603_1608Metric",
    pin_defs={"1": "1", "2": "2"},
    pins={"1": usb.D.P, "2": usb.D.N},
)
