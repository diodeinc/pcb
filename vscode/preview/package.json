    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/pdfkit": "^0.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vscode-elements/elements": "^1.14.0",
    "@xyflow/react": "^12.4.4",
    "canvas": "^2.11.2",
    "elkjs": "^0.10.0",
    "jspdf": "^3.0.1",
    "libavoid-js": "github:LK/libavoid-js#f994097",
    "pdfkit": "^0.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-feather": "^2.0.10",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "postbuild": "for dir in ./playwright/.cache ./public ./build ./build/static/js; do cp -f node_modules/libavoid-js/dist/libavoid.wasm $dir/libavoid.wasm; done",
    "test": "craco test --watchAll=false",
    "test:watch": "craco test",
    "test:visual": "playwright test -c playwright-ct.config.ts",
    "test:visual:ui": "playwright test -c playwright-ct.config.ts --ui",
    "test:all": "npm run test && npm run test:visual",
    "eject": "react-scripts eject",
    "render-symbol": "ts-node --project scripts/tsconfig.json scripts/render-kicad-sym.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@playwright/experimental-ct-react": "^1.53.2",
    "@playwright/test": "^1.53.2",
    "@types/lodash": "^4.17.16",
    "@types/puppeteer": "^7.0.4",
    "copy-webpack-plugin": "^13.0.0",
    "esbuild": "^0.25.4",
    "puppeteer": "^24.9.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2"
  }
}
