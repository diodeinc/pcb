# Simple circuit for demonstrating expected failures
load("@stdlib/interfaces.zen", "Power", "Ground")

Resistor = Module("@stdlib/generics/Resistor.zen")
Capacitor = Module("@stdlib/generics/Capacitor.zen")

vcc = Power("VCC")
gnd = Ground("GND")

# Simple RC circuit: VCC -> R1(1k) -> C1(100nF) -> GND
Resistor(name="R1", value="1kOhm", package="0603", P1=vcc.NET, P2=Net("MID"))
Capacitor(name="C1", value="100nF", package="0402", P1=Net("MID"), P2=gnd.NET)

VCC = io("VCC", Power, default=vcc)
GND = io("GND", Ground, default=gnd)
