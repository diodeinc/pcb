# Comprehensive Test Circuit - showcasing various component patterns
load("@stdlib/interfaces.zen", "Power", "Ground")

Resistor = Module("@stdlib/generics/Resistor.zen")
Capacitor = Module("@stdlib/generics/Capacitor.zen")
Inductor = Module("@stdlib/generics/Inductor.zen")

# Create a complex but working circuit
vcc = Power("VCC")
gnd = Ground("GND")

# Chain 1: RC Low-pass filter
# VCC -> R1(1k) -> C1(100nF) -> GND
Resistor(name="R1", value="1kOhm", package="0603", P1=vcc.NET, P2=Net("N1"))
Capacitor(name="C1", value="100nF", package="0402", P1=Net("N1"), P2=gnd.NET)

# Chain 2: Power supply decoupling  
# VCC -> C2(10uF) -> GND
# VCC -> C3(1uF) -> GND  
Capacitor(name="C2", value="10uF", package="0805", P1=vcc.NET, P2=gnd.NET)
Capacitor(name="C3", value="1uF", package="0603", P1=vcc.NET, P2=gnd.NET)

# Chain 3: Complex filter with inductor
# VCC -> R2(100) -> L1(10uH) -> R3(10k) -> C4(470nF) -> GND
Resistor(name="R2", value="100Ohm", package="0603", P1=vcc.NET, P2=Net("N2"))
Inductor(name="L1", value="10uH", package="0805", P1=Net("N2"), P2=Net("N3"))
Resistor(name="R3", value="10kOhm", package="0603", P1=Net("N3"), P2=Net("N4"))
Capacitor(name="C4", value="470nF", package="0603", P1=Net("N4"), P2=gnd.NET)

# Chain 4: Voltage divider
# VCC -> R4(2.2k) -> R5(1k) -> GND  
Resistor(name="R4", value="2.2kOhm", package="0402", P1=vcc.NET, P2=Net("N5"))
Resistor(name="R5", value="1kOhm", package="0402", P1=Net("N5"), P2=gnd.NET)

VCC = io("VCC", Power, default=vcc)
GND = io("GND", Ground, default=gnd)
