load("@stdlib:v0.2.8/interfaces.zen", "Ground", "Power")
Resistor = Module("@stdlib:v0.2.8/generics/Resistor.zen")
Capacitor = Module("@stdlib:v0.2.8/generics/Capacitor.zen")

vcc = io("VCC", Power)
gnd = io("GND", Ground)
vcc2 = Power("VCC2")
gnd2 = Ground("GND2")

Resistor(
    name="R1",
    value="1kOhm",
    package="0402",
    P1=vcc2,
    P2=gnd2,
)

Resistor(
    name="R2",
    value="10kOhm",
    package="0402",
    P1=vcc,
    P2=gnd,
)

Resistor(
    name="R3",
    value="100kOhm",
    package="0402",
    P1=vcc,
    P2=gnd,
)
